{
  "name": "gotpl",
  "version": "8.2.2",
  "homepage": "https://github.com/Lanfei/gotpl",
  "description": "A lightweight, high-performance JavaScript template engine.",
  "scripts": {
    "test": "npm run build && mocha -r expect.js test/index.js",
    "build": "rimraf dist && rollup -c && npm run uglify",
    "uglify": "uglifyjs dist/gotpl.js -o dist/gotpl.min.js -c --source-map"
  },
  "author": {
    "name": "Lanfei",
    "url": "http://www.clanfei.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lanfei/gotpl.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.10.0"
  },
  "keywords": [
    "template",
    "tpl"
  ],
  "main": "dist/gotpl.common.js",
  "dependencies": {
    "escape-html": "^1.0.3",
    "is-keyword-js": "^1.0.3",
    "js-tokens": "^4.0.0"
  },
  "devDependencies": {
    "expect.js": "^0.3.1",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.2",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "uglify-js": "^3.4.9"
  }
